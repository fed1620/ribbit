<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#26833a" />
            <stroke
                android:width="1dp"
                android:color="#53933f" />
            <corners
                android:radius="9dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#30a349"/>
            <stroke
                android:width="1dp"
                android:color="#53933f" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
</selector>